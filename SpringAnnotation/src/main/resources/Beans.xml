<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config/>
   <!-- bean definitions go here -->

    <bean id="student" class="fan.stu.springannotation.Student" init-method="init" destroy-method="destroy">
        <property name="name" value="Phan Ngoc Thao"></property>
        <property name="age" value="23"></property>
    </bean>

    <bean id="textEditor" class="fan.stu.springannotation.TextEditor">
    </bean>

    <bean id="spellChecker1" class="fan.stu.springannotation.SpellChecker">
    </bean>
    <bean id="spellChecker2" class="fan.stu.springannotation.SpellChecker">
    </bean>

</beans>